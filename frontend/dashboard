**src/components/ResourceList.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchResources } from "../api/api";
import {
  Paper,
  Table,
  TableBody,
  TableCell,
  TableContainer,
  TableHead,
  TableRow,
  Typography,
  CircularProgress,
} from "@mui/material";

type Resource = {
  id: number;
  name: string;
  type: string;
  cloud_provider: string;
  region?: string;
  onboarded: boolean;
};

const ResourceList: React.FC = () => {
  const [resources, setResources] = useState<Resource[]>([]);
  const [loading, setLoading] = useState<boolean>(true);

  useEffect(() => {
    fetchResources()
      .then((res) => setResources(res.data))
      .catch(() => setResources([]))
      .finally(() => setLoading(false));
  }, []);

  if (loading) return <CircularProgress />;

  return (
    <Paper sx={{ p: 2 }}>
      <Typography variant="h6" gutterBottom>
        Monitored Resources
      </Typography>
      <TableContainer>
        <Table size="small">
          <TableHead>
            <TableRow>
              <TableCell>Name</TableCell>
              <TableCell>Type</TableCell>
              <TableCell>Cloud Provider</TableCell>
              <TableCell>Region</TableCell>
              <TableCell>Onboarded</TableCell>
            </TableRow>
          </TableHead>
          <TableBody>
            {resources.map((r) => (
              <TableRow key={r.id}>
                <TableCell>{r.name}</TableCell>
                <TableCell>{r.type}</TableCell>
                <TableCell>{r.cloud_provider}</TableCell>
                <TableCell>{r.region}</TableCell>
                <TableCell>{r.onboarded ? "Yes" : "No"}</TableCell>
              </TableRow>
            ))}
          </TableBody>
        </Table>
      </TableContainer>
    </Paper>
  );
};

export default ResourceList;
```

---

**src/components/MetricsChart.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchResourceMetrics, fetchResources } from "../api/api";
import {
  Paper,
  Typography,
  Select,
  MenuItem,
  FormControl,
  InputLabel,
  CircularProgress,
  Box,
} from "@mui/material";
import { Line } from "react-chartjs-2";
import { Chart, LineElement, PointElement, LinearScale, CategoryScale, Legend, Tooltip } from "chart.js";

Chart.register(LineElement, PointElement, LinearScale, CategoryScale, Legend, Tooltip);

type Resource = {
  id: number;
  name: string;
};

const MetricsChart: React.FC = () => {
  const [resources, setResources] = useState<Resource[]>([]);
  const [selected, setSelected] = useState<number | null>(null);
  const [metrics, setMetrics] = useState<any>(null);
  const [loading, setLoading] = useState<boolean>(false);

  useEffect(() => {
    fetchResources()
      .then((res) => setResources(res.data))
      .catch(() => setResources([]));
  }, []);

  useEffect(() => {
    if (selected !== null) {
      setLoading(true);
      fetchResourceMetrics(selected)
        .then((res) => setMetrics(res.data.metrics))
        .catch(() => setMetrics(null))
        .finally(() => setLoading(false));
    }
  }, [selected]);

  const metricNames = ["cpu", "memory", "network", "storage"];

  return (
    <Paper sx={{ p: 2, mb: 2 }}>
      <Typography variant="h6" gutterBottom>
        Resource Metrics
      </Typography>
      <FormControl sx={{ minWidth: 200, mb: 2 }}>
        <InputLabel>Select Resource</InputLabel>
        <Select
          value={selected ?? ""}
          label="Select Resource"
          onChange={(e) => setSelected(Number(e.target.value))}
        >
          {resources.map((r) => (
            <MenuItem key={r.id} value={r.id}>
              {r.name}
            </MenuItem>
          ))}
        </Select>
      </FormControl>
      {loading && <CircularProgress />}
      {metrics && (
        <Box>
          {metricNames.map((metric) =>
            metrics[metric] ? (
              <Box key={metric} sx={{ mb: 3 }}>
                <Typography variant="subtitle1">{metric.toUpperCase()}</Typography>
                <Line
                  data={{
                    labels: metrics[metric].map((_: any, idx: number) => idx),
                    datasets: [
                      {
                        label: metric,
                        data: metrics[metric].map((m: any) =>
                          Array.isArray(m.value) ? Number(m.value[1]) : Number(m.value || m.Average || m.Value || 0)
                        ),
                        fill: false,
                        borderColor: "#1976d2",
                        tension: 0.1,
                      },
                    ],
                  }}
                  options={{
                    responsive: true,
                    plugins: { legend: { display: false } },
                  }}
                />
              </Box>
            ) : null
          )}
        </Box>
      )}
    </Paper>
  );
};

export default MetricsChart;
```

---

**src/components/AlertsTable.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchAlerts, resolveAlert } from "../api/api";
import {
  Paper,
  Table,
  TableBody,
  TableCell,
  TableContainer,
  TableHead,
  TableRow,
  Typography,
  Button,
  CircularProgress,
} from "@mui/material";

type Alert = {
  id: number;
  resource_id?: number;
  severity: string;
  status: string;
  message: string;
  triggered_at: string;
  channel?: string;
};

const AlertsTable: React.FC = () => {
  const [alerts, setAlerts] = useState<Alert[]>([]);
  const [loading, setLoading] = useState<boolean>(true);

  const loadAlerts = () => {
    setLoading(true);
    fetchAlerts()
      .then((res) => setAlerts(res.data))
      .catch(() => setAlerts([]))
      .finally(() => setLoading(false));
  };

  useEffect(() => {
    loadAlerts();
  }, []);

  const handleResolve = (alertId: number) => {
    resolveAlert(alertId, { status: "resolved", resolver: "dashboard" }).then(loadAlerts);
  };

  if (loading) return <CircularProgress />;

  return (
    <Paper sx={{ p: 2 }}>
      <Typography variant="h6" gutterBottom>
        Active Alerts
      </Typography>
      <TableContainer>
        <Table size="small">
          <TableHead>
            <TableRow>
              <TableCell>Resource ID</TableCell>
              <TableCell>Severity</TableCell>
              <TableCell>Status</TableCell>
              <TableCell>Message</TableCell>
              <TableCell>Triggered At</TableCell>
              <TableCell>Channel</TableCell>
              <TableCell>Action</TableCell>
            </TableRow>
          </TableHead>
          <TableBody>
            {alerts.map((a) => (
              <TableRow key={a.id}>
                <TableCell>{a.resource_id}</TableCell>
                <TableCell>{a.severity}</TableCell>
                <TableCell>{a.status}</TableCell>
                <TableCell>{a.message}</TableCell>
                <TableCell>{new Date(a.triggered_at).toLocaleString()}</TableCell>
                <TableCell>{a.channel}</TableCell>
                <TableCell>
                  {a.status === "active" && (
                    <Button
                      variant="contained"
                      color="success"
                      size="small"
                      onClick={() => handleResolve(a.id)}
                    >
                      Resolve
                    </Button>
                  )}
                </TableCell>
              </TableRow>
            ))}
          </TableBody>
        </Table>
      </TableContainer>
    </Paper>
  );
};

export default AlertsTable;
```

---

**src/components/IncidentTable.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchIncidents } from "../api/api";
import {
  Paper,
  Table,
  TableBody,
  TableCell,
  TableContainer,
  TableHead,
  TableRow,
  Typography,
  CircularProgress,
} from "@mui/material";

type Incident = {
  id: number;
  alert_id: number;
  resource_id?: number;
  status: string;
  description?: string;
  created_at: string;
  resolved_at?: string;
  resolver?: string;
};

const IncidentTable: React.FC = () => {
  const [incidents, setIncidents] = useState<Incident[]>([]);
  const [loading, setLoading] = useState<boolean>(true);

  useEffect(() => {
    fetchIncidents()
      .then((res) => setIncidents(res.data))
      .catch(() => setIncidents([]))
      .finally(() => setLoading(false));
  }, []);

  if (loading) return <CircularProgress />;

  return (
    <Paper sx={{ p: 2 }}>
      <Typography variant="h6" gutterBottom>
        Incidents
      </Typography>
      <TableContainer>
        <Table size="small">
          <TableHead>
            <TableRow>
              <TableCell>Alert ID</TableCell>
              <TableCell>Resource ID</TableCell>
              <TableCell>Status</TableCell>
              <TableCell>Description</TableCell>
              <TableCell>Created At</TableCell>
              <TableCell>Resolved At</TableCell>
              <TableCell>Resolver</TableCell>
            </TableRow>
          </TableHead>
          <TableBody>
            {incidents.map((i) => (
              <TableRow key={i.id}>
                <TableCell>{i.alert_id}</TableCell>
                <TableCell>{i.resource_id}</TableCell>
                <TableCell>{i.status}</TableCell>
                <TableCell>{i.description}</TableCell>
                <TableCell>{new Date(i.created_at).toLocaleString()}</TableCell>
                <TableCell>{i.resolved_at ? new Date(i.resolved_at).toLocaleString() : ""}</TableCell>
                <TableCell>{i.resolver}</TableCell>
              </TableRow>
            ))}
          </TableBody>
        </Table>
      </TableContainer>
    </Paper>
  );
};

export default IncidentTable;
```

