---

**src/api/api.ts**
```typescript
import axios from "axios";

const API_BASE = process.env.REACT_APP_API_BASE || "http://localhost:8000";

export const fetchResources = () => axios.get(`${API_BASE}/resources`);
export const fetchResourceMetrics = (resourceId: number) =>
  axios.get(`${API_BASE}/resources/${resourceId}/metrics`);
export const fetchAlerts = () => axios.get(`${API_BASE}/alerts`);
export const fetchIncidents = () => axios.get(`${API_BASE}/incidents`);
export const fetchAuditTrail = (limit: number = 100) =>
  axios.get(`${API_BASE}/audit?limit=${limit}`);
export const fetchOnboardingStatus = () =>
  axios.get(`${API_BASE}/onboarding/status`);
export const fetchSecurityEvents = (limit: number = 100) =>
  axios.get(`${API_BASE}/audit?limit=${limit}`); // Filter on frontend for security events

export const resolveAlert = (alertId: number, data: any) =>
  axios.patch(`${API_BASE}/alerts/${alertId}/resolve`, data);

export const triggerAlertCheck = (resourceId: number) =>
  axios.post(`${API_BASE}/alerts/check/${resourceId}`);

export const onboardResource = (resource: any) =>
  axios.post(`${API_BASE}/resources`, resource);

export const sendTestAlert = (channel: string) =>
  axios.post(`${API_BASE}/alerts/test?channel=${channel}`);

export const notifyDevOps = (issue: any) =>
  axios.post(`${API_BASE}/notify/devops`, issue);
```

