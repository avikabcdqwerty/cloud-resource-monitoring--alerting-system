**src/components/OnboardingStatus.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchOnboardingStatus } from "../api/api";
import { Paper, Typography, CircularProgress, Box, LinearProgress } from "@mui/material";

type Status = {
  total_resources: number;
  onboarded: number;
  not_onboarded: number;
  status: string;
};

const OnboardingStatus: React.FC = () => {
  const [status, setStatus] = useState<Status | null>(null);
  const [loading, setLoading] = useState<boolean>(true);

  useEffect(() => {
    fetchOnboardingStatus()
      .then((res) => setStatus(res.data))
      .catch(() => setStatus(null))
      .finally(() => setLoading(false));
  }, []);

  if (loading) return <CircularProgress />;

  return (
    <Paper sx={{ p: 2, mb: 2 }}>
      <Typography variant="h6" gutterBottom>
        Resource Onboarding Status
      </Typography>
      {status && (
        <Box>
          <Typography>
            Onboarded: {status.onboarded} / {status.total_resources}
          </Typography>
          <LinearProgress
            variant="determinate"
            value={
              status.total_resources > 0
                ? (status.onboarded / status.total_resources) * 100
                : 0
            }
            sx={{ my: 1 }}
          />
          <Typography>Status: {status.status}</Typography>
        </Box>
      )}
    </Paper>
  );
};

export default OnboardingStatus;
```

---

**src/components/SecurityEvents.tsx**
```tsx
import React, { useEffect, useState } from "react";
import { fetchAuditTrail } from "../api/api";
import {
  Paper,
  Table,
  TableBody,
  TableCell,
  TableContainer,
  TableHead,
  TableRow,
  Typography,
  CircularProgress,
} from "@mui/material";

const SECURITY_EVENT_TYPES = ["security_event", "CONFIG_CHANGE", "unauthorized_access"];

type AuditLog = {
  id: number;
  event_type: string;
  details?: string;
  created_at: string;
  actor?: string;
};

const SecurityEvents: React.FC = () => {
  const [logs, setLogs] = useState<AuditLog[]>([]);
  const [loading, setLoading] = useState<boolean>(true);

  useEffect(() => {
    fetchAuditTrail()
      .then((res) =>
        setLogs(
          res.data.filter((log: AuditLog) =>
            SECURITY_EVENT_TYPES.includes(log.event_type.toLowerCase())
          )
        )
      )
      .catch(() => setLogs([]))
      .finally(() => setLoading(false));
  }, []);

  if (loading) return <CircularProgress />;

  return (
    <Paper sx={{ p: 2 }}>
      <Typography variant="h6" gutterBottom>
        Security Events
      </Typography>
      <TableContainer>
        <Table size="small">
          <TableHead>
            <TableRow>
              <TableCell>Event Type</TableCell>
              <TableCell>Details</TableCell>
              <TableCell>Created At</TableCell>
              <TableCell>Actor</TableCell>
            </TableRow>
          </TableHead>
          <TableBody>
            {logs.map((log) => (
              <TableRow key={log.id}>
                <TableCell>{log.event_type}</TableCell>
                <TableCell>{log.details}</TableCell>
                <TableCell>{new Date(log.created_at).toLocaleString()}</TableCell>
                <TableCell>{log.actor}</TableCell>
              </TableRow>
            ))}
          </TableBody>
        </Table>
      </TableContainer>
    </Paper>
  );
};

export default SecurityEvents;
```

